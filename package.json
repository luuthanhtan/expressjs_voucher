{
  "name": "nodejs_training",
  "version": "0.0.0",
  "scripts": {
    "build": "tsc",
    "clean-install": "rm -rf ./node_modules && rm -r package-lock.json && npm i",
    "dev": "cross-env NODE_ENV=development ts-node ./index.ts",
    "dev:hot": "nodemon --exec \"npm run dev\" --watch ./src --ext .ts",
    "lint": "eslint .",
    "start": "cross-env NODE_ENV=production node -r ./config.js ./dist/src/index.js",
    "type-check": "tsc --noEmit",
    "routes": "ts-node scripts/routes_list.ts",
    "seed": "ts-node src/seeders/index.ts",
    "seed:users": "ts-node src/seeders/seed-user.ts",
    "seed:events": "ts-node src/seeders/seed-event.ts",
    "seed:vouchers": "ts-node src/seeders/seed-voucher.ts",
    "watch": "cross-env NODE_ENV=development nodemon"
  },
  "engines": {
    "node": ">=16.0.0"
  },
  "dependencies": {
    "@bull-board/api": "^6.10.1",
    "@bull-board/express": "^6.10.1",
    "@bull-board/ui": "^6.10.1",
    "agenda": "^5.0.0",
    "bcrypt": "^6.0.0",
    "bullmq": "^5.56.0",
    "cookie-parser": "^1.4.7",
    "cors": "^2.8.5",
    "dayjs": "^1.11.13",
    "dotenv": "^16.5.0",
    "express": "^5.1.0",
    "helmet": "^8.1.0",
    "inserturlparams": "^2.0.5",
    "ioredis": "^5.6.1",
    "jet-env": "^1.1.4",
    "jet-logger": "^2.0.1",
    "jet-paths": "^1.1.0",
    "jet-validators": "^1.4.2",
    "joi": "^17.13.3",
    "jsonfile": "^6.1.0",
    "jsonwebtoken": "^9.0.2",
    "module-alias": "^2.2.3",
    "mongoose": "^8.15.1",
    "morgan": "^1.10.0",
    "nodemailer": "^7.0.3",
    "swagger-jsdoc": "^6.2.8",
    "swagger-ui-express": "^5.0.1"
  },
  "devDependencies": {
    "@eslint/js": "^9.28.0",
    "@stylistic/eslint-plugin-js": "^4.4.0",
    "@stylistic/eslint-plugin-ts": "^4.4.0",
    "@swc/core": "^1.11.29",
    "@types/bcrypt": "^5.0.2",
    "@types/cookie-parser": "^1.4.8",
    "@types/cors": "^2.8.19",
    "@types/find": "^0.2.4",
    "@types/fs-extra": "^11.0.4",
    "@types/jest": "^30.0.0",
    "@types/jsonfile": "^6.1.4",
    "@types/jsonwebtoken": "^9.0.9",
    "@types/module-alias": "^2.0.4",
    "@types/morgan": "^1.9.9",
    "@types/node": "^22.15.29",
    "@types/nodemailer": "^6.4.17",
    "@types/supertest": "^6.0.3",
    "@types/swagger-jsdoc": "^6.0.4",
    "@types/swagger-ui-express": "^4.1.8",
    "cross-env": "^7.0.3",
    "eslint": "^9.28.0",
    "eslint-plugin-n": "^17.19.0",
    "express-list-endpoints": "^7.1.1",
    "find": "^0.3.0",
    "fs-extra": "^11.3.0",
    "jest": "^30.0.4",
    "jiti": "^2.4.2",
    "migrate-mongo": "^12.1.3",
    "nodemon": "^3.1.10",
    "supertest": "^7.1.1",
    "ts-jest": "^29.4.0",
    "ts-node": "^10.9.2",
    "tsconfig-paths": "^4.2.0",
    "typescript": "^5.8.3",
    "typescript-eslint": "^8.33.1",
    "vitest": "^3.2.2"
  }
}
